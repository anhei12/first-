#include <stdio.h>


int ss(int a)//ss:素数
{
    int i;
        for(i=2;i<a;++i)
    {

        if(a%i==0)
            break;
    }
    if(i==a)
        return 1;
    else
        return 0;
}
int main()
{
    int a;
    printf("请输入：");
    scanf("%d",&a);
    if(ss(a)==1)
        printf("是素数");
    else
        printf("不是素数");
   return 0;
}
