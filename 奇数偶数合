#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int sum1=0;
    int sum2=0;
    int sum=0;
    for(i=1;i<=100;i++)
    {
        if(i%2==1)
           sum1=sum1+1;

    }
    for(i=1;i<=100;i++)
    {
        if(i%2==0)
           sum2=sum2+1;

    }
    sum=sum1+sum2;


    printf("sum=%d\n",sum);
    return 0;
}
