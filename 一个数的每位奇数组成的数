#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a;
    int b=0;//每位数的值
    int c=0;
    int d=0;//输出的值
    printf("请输入：");
    scanf("%d",&a);
    while(a)
    {
        b=a%10;
        a=a/10;
        if(b%2)
        {
            c=c*10+b;
        }

    }
    
    while(c)
    {
        d=d*10+c%10;
        c=c/10;
    }
    printf("%d",d);
    
    return 0;
}
